{
	"name": "pln_master",
	"properties": {
		"activities": [
			{
				"name": "Load Horizon data",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Send progressing to Horizon data",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "Record moving on to loading Horizon data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": true
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "pln_all_horizon_data",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"webhook_url": {
							"value": "@variables('webhook_url')",
							"type": "Expression"
						},
						"subscription_id": {
							"value": "@variables('subscription_id')",
							"type": "Expression"
						},
						"resource_group": {
							"value": "@variables('resource_group')",
							"type": "Expression"
						},
						"starting_colour": {
							"value": "@variables('starting_colour')",
							"type": "Expression"
						},
						"on_progress_colour": {
							"value": "@variables('on_progress_colour')",
							"type": "Expression"
						},
						"failed_colour": {
							"value": "@variables('failed_colour')",
							"type": "Expression"
						},
						"starting_image": {
							"value": "@variables('starting_image')",
							"type": "Expression"
						},
						"progress_image": {
							"value": "@variables('progress_image')",
							"type": "Expression"
						},
						"warning_image": {
							"value": "@variables('warning_image')",
							"type": "Expression"
						},
						"failed_image": {
							"value": "@variables('failed_image')",
							"type": "Expression"
						},
						"warning_colour": {
							"value": "@variables('warning_colour')",
							"type": "Expression"
						},
						"apps_insights_ikey": {
							"value": "@variables('apps_insights_ikey')",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "Service bus loads",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Record moving on to loading service bus data",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "Send progressing to service bus data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('service_bus_feed')",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "Random wait to allow parallelism_copy4",
							"type": "Wait",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": {
									"value": "@rand(1, 20)",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Execute Service Bus Pipeline",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "Random wait to allow parallelism_copy4",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "pln_service_bus",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"entity_name_input": {
										"value": "@item()",
										"type": "Expression"
									},
									"apps_insights_ikey": {
										"value": "@variables('apps_insights_ikey')",
										"type": "Expression"
									}
								}
							}
						},
						{
							"name": "Record Service Bus failure",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "Execute Service Bus Pipeline",
									"dependencyConditions": [
										"Failed"
									]
								}
							],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "pln_log_to_appins",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"Stage": "Fail",
									"PipelineName": {
										"value": "@activity('Execute Service Bus Pipeline').Output.pipelineName",
										"type": "Expression"
									},
									"PipelineRunID": {
										"value": "@activity('Execute Service Bus Pipeline').Output.pipelineRunId",
										"type": "Expression"
									},
									"StartTime": {
										"value": "@activity('Execute Service Bus Pipeline').ExecutionStartTime",
										"type": "Expression"
									},
									"EndTime": {
										"value": "@activity('Execute Service Bus Pipeline').ExecutionEndTime",
										"type": "Expression"
									},
									"ErrorMessage": {
										"value": "@activity('Execute Service Bus Pipeline').Error.message",
										"type": "Expression"
									},
									"StatusMessage": {
										"value": "@concat(item(), ': ', activity('Execute Service Bus Pipeline').Status)",
										"type": "Expression"
									},
									"PipelineTriggeredbyPipelineName": {
										"value": "@activity('Execute Service Bus Pipeline').PipelineName",
										"type": "Expression"
									},
									"PipelineTriggeredbyPipelineRunID": {
										"value": "@activity('Execute Service Bus Pipeline').PipelineRunId",
										"type": "Expression"
									},
									"PipelineExecutionTimeInSec": {
										"value": "@activity('Execute Service Bus Pipeline').Duration",
										"type": "Expression"
									},
									"ActivityType": {
										"value": "@activity('Execute Service Bus Pipeline').ActivityType",
										"type": "Expression"
									},
									"DurationSeconds": {
										"value": "@activity('Execute Service Bus Pipeline').Duration",
										"type": "Expression"
									},
									"AppInsCustomEventName": "ODW_Master_Pipeline_Logs",
									"StatusCode": {
										"value": "@activity('Execute Service Bus Pipeline').StatusCode",
										"type": "Expression"
									},
									"AppInsIKey": {
										"value": "@variables('apps_insights_ikey')",
										"type": "Expression"
									}
								}
							}
						},
						{
							"name": "Return Failure",
							"description": "Return as a failed activity after logging the error",
							"type": "Fail",
							"dependsOn": [
								{
									"activity": "Record Service Bus failure",
									"dependencyConditions": [
										"Completed"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"message": {
									"value": "@activity('Execute Service Bus Pipeline').Error.message",
									"type": "Expression"
								},
								"errorCode": {
									"value": "@activity('Execute Service Bus Pipeline').StatusCode",
									"type": "Expression"
								}
							}
						}
					]
				}
			},
