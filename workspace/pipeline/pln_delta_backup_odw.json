{
	"name": "pln_delta_backup_odw",
	"properties": {
		"activities": [
			{
				"name": "non_delta_directories",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "delta_back_odw",
						"type": "NotebookReference"
					},
					"snapshot": true,
					"conf": {
						"spark.dynamicAllocation.enabled": null,
						"spark.dynamicAllocation.minExecutors": null,
						"spark.dynamicAllocation.maxExecutors": null
					},
					"numExecutors": null
				}
			},
			{
				"name": "ForEach_deltaFile",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "non_delta_directories",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@json(replace(replace(activity('non_delta_directories').output.status.Output.result.exitValue, '''', '\"'), '\\\"[', '['))\n\n",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "file_name_variable",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "file_name",
								"value": {
									"value": "@item()",
									"type": "Expression"
								}
							}
						},
						{
							"name": "container_name",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "container_name",
								"value": {
									"value": "@split(split(item(), '://')[1], '@')[0]",
									"type": "Expression"
								}
							}
						},
						{
							"name": "storage_account",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "storage_account_name",
								"value": {
									"value": "@split(split(split(item(), '://')[1], '@')[1], '.')[0]\n",
									"type": "Expression"
								}
							}
						},
						{
							"name": "table_name",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "table_name",
								"value": {
									"value": "@last(split(item(), '/'))",
									"type": "Expression"
								}
							}
						},
						{
							"name": "dynamic_copy_data",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "BinarySource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"deleteFilesAfterCompletion": false
									},
									"formatSettings": {
										"type": "BinaryReadSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "ds_dynamic_delta_backup",
									"type": "DatasetReference",
									"parameters": {
										"container_name": {
											"value": "@split(split(item(), '://')[1], '@')[0]",
											"type": "Expression"
										},
										"table_name": {
											"value": "@last(split(item(), '/'))",
											"type": "Expression"
										},
										"storage_account_name": {
											"value": "@split(split(split(item(), '://')[1], '@')[1], '.')[0]",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"variables": {
			"file_path_array": {
				"type": "Array"
			},
			"file_name": {
				"type": "String"
			},
			"container_name": {
				"type": "String"
			},
			"storage_account_name": {
				"type": "String"
			},
			"table_name": {
				"type": "String"
			}
		},
		"annotations": []
	}
}