{
	"name": "pln_delta_backup_odw",
	"properties": {
		"activities": [
			{
				"name": "non_delta_directories",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "delta_back_odw",
						"type": "NotebookReference"
					},
					"snapshot": true,
					"conf": {
						"spark.dynamicAllocation.enabled": null,
						"spark.dynamicAllocation.minExecutors": null,
						"spark.dynamicAllocation.maxExecutors": null
					},
					"numExecutors": null
				}
			},
			{
				"name": "ForEach_deltaFile",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "non_delta_directories",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@json(replace(replace(activity('non_delta_directories').output.status.Output.result.exitValue, '''', '\"'), '\\\"[', '['))\n\n",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "file_name_variable",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "file_name",
								"value": {
									"value": "@item()",
									"type": "Expression"
								}
							}
						},
						{
							"name": "backup_odw_standardised",
							"type": "Copy",
							"state": "Inactive",
							"onInactiveMarkAs": "Succeeded",
							"dependsOn": [
								{
									"activity": "file_name_variable",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "odw-standardised//"
								},
								{
									"name": "Destination",
									"value": "odw-standardised//"
								}
							],
							"typeProperties": {
								"source": {
									"type": "BinarySource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"wildcardFileName": "*"
									},
									"formatSettings": {
										"type": "BinaryReadSettings"
									}
								},
								"sink": {
									"type": "BinarySink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings",
										"copyBehavior": "PreserveHierarchy"
									}
								},
								"enableStaging": false,
								"preserve": [
									"ACL",
									"Owner",
									"Group",
									"Attributes"
								],
								"enableSkipIncompatibleRow": false,
								"skipErrorFile": {
									"fileMissing": true,
									"dataInconsistency": true
								},
								"validateDataConsistency": true,
								"logSettings": {
									"enableCopyActivityLog": true,
									"copyActivityLogSettings": {
										"logLevel": "Warning",
										"enableReliableLogging": true
									},
									"logLocationSettings": {
										"linkedServiceName": {
											"referenceName": "ls_backup_source",
											"type": "LinkedServiceReference"
										},
										"path": "backup-logs"
									}
								}
							},
							"inputs": [
								{
									"referenceName": "bkp_src_odw_standardised",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "bkp_dst_odw_standardised",
									"type": "DatasetReference"
								}
							]
						},
						{
							"name": "container_name",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "container_name",
								"value": {
									"value": "@split(split(item(), '://')[1], '@')[0]",
									"type": "Expression"
								}
							}
						},
						{
							"name": "storage_account",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "storage_account_name",
								"value": {
									"value": "@split(split(split(item(), '://')[1], '@')[1], '.')[0]\n",
									"type": "Expression"
								}
							}
						},
						{
							"name": "table_name",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "table_name",
								"value": {
									"value": "@last(split(item(), '/'))",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"variables": {
			"file_path_array": {
				"type": "Array"
			},
			"file_name": {
				"type": "String"
			},
			"container_name": {
				"type": "String"
			},
			"storage_account_name": {
				"type": "String"
			},
			"table_name": {
				"type": "String"
			}
		},
		"annotations": []
	}
}