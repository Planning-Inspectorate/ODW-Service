{
	"name": "pln_hr_measures_schedule",
	"properties": {
		"activities": [
			{
				"name": "Lookup New Data In Standardised",
				"type": "SynapseNotebook",
				"dependsOn": [
					{
						"activity": "py_hr_monthly_raw_to_std",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "mipins_hr_measures_new_data_lookup",
						"type": "NotebookReference"
					},
					"parameters": {
						"month": {
							"value": {
								"value": "@formatDateTime(utcnow(), 'MM')",
								"type": "Expression"
							},
							"type": "int"
						},
						"year": {
							"value": {
								"value": "@formatDateTime(utcnow(), 'yyyy')",
								"type": "Expression"
							},
							"type": "int"
						}
					},
					"snapshot": true,
					"conf": {
						"spark.dynamicAllocation.enabled": null,
						"spark.dynamicAllocation.minExecutors": null,
						"spark.dynamicAllocation.maxExecutors": null
					},
					"numExecutors": null
				}
			},
			{
				"name": "If New Data Exists",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "Lookup New Data In Standardised",
						"dependencyConditions": [
							"Completed"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@activity('Lookup New Data In Standardised').output.status.Output.result.exitValue",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "New_Data_Not_Found",
							"type": "SynapseNotebook",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebook": {
									"referenceName": "py_fail_activity_logging",
									"type": "NotebookReference"
								},
								"snapshot": true,
								"conf": {
									"spark.dynamicAllocation.enabled": null,
									"spark.dynamicAllocation.minExecutors": null,
									"spark.dynamicAllocation.maxExecutors": null
								},
								"numExecutors": null
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "py_harmonised_and_hr_measures_monthly",
							"type": "SynapseNotebook",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebook": {
									"referenceName": "py_harmonised_and_hr_measures_monthly",
									"type": "NotebookReference"
								},
								"parameters": {
									"start_month": {
										"value": {
											"value": "@formatDateTime(utcnow(), 'MM')",
											"type": "Expression"
										},
										"type": "int"
									},
									"end_month": {
										"value": {
											"value": "@formatDateTime(utcnow(), 'MM')",
											"type": "Expression"
										},
										"type": "int"
									},
									"start_year": {
										"value": {
											"value": "@formatDateTime(utcnow(), 'yyyy')",
											"type": "Expression"
										},
										"type": "int"
									},
									"end_year": {
										"value": {
											"value": "@formatDateTime(utcnow(), 'yyyy')",
											"type": "Expression"
										},
										"type": "int"
									}
								},
								"snapshot": true,
								"conf": {
									"spark.dynamicAllocation.enabled": null,
									"spark.dynamicAllocation.minExecutors": null,
									"spark.dynamicAllocation.maxExecutors": null
								},
								"numExecutors": null
							}
						}
					]
				}
			},
			{
				"name": "py_hr_monthly_raw_to_std",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "py_hr_monthly_raw_to_std",
						"type": "NotebookReference"
					},
					"parameters": {
						"specific_folder": {
							"value": {
								"value": "@concat(formatDateTime(utcnow(), 'yyyy'), '-', formatDateTime(utcnow(), 'MM'))",
								"type": "Expression"
							},
							"type": "string"
						}
					},
					"snapshot": true,
					"conf": {
						"spark.dynamicAllocation.enabled": null,
						"spark.dynamicAllocation.minExecutors": null,
						"spark.dynamicAllocation.maxExecutors": null
					},
					"numExecutors": null
				}
			}
		],
		"folder": {
			"name": "a-resources/hr/utils"
		},
		"annotations": []
	}
}