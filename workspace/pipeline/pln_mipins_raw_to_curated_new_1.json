{
	"name": "pln_mipins_raw_to_curated_new_1",
	"properties": {
		"activities": [
			{
				"name": "Create the table lookup",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Truncate logging table",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ds_odw_mipins_load_tables",
						"type": "DatasetReference",
						"parameters": {
							"source_schema_name": "odw",
							"source_table_name": "load_tables"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_odw_mipins_config",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Lookup MiPINS table list",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Create the table lookup",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "ds_odw_mipins_config",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Raw to Curated for reports",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Lookup MiPINS table list",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Lookup MiPINS table list').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "py_process_raw_to_curated_table_for_reports",
							"type": "SynapseNotebook",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"typeProperties": {
								"notebook": {
									"referenceName": "py_process_raw_to_curated_table_for_reports",
									"type": "NotebookReference"
								},
								"parameters": {
									"raw_storage_container": {
										"value": "abfss://odw-raw@pinsodwdatadevstorage.dfs.core.windows.net",
										"type": "string"
									},
									"raw_storage_source_folder": {
										"value": {
											"value": "@item().LandingFolder",
											"type": "Expression"
										},
										"type": "string"
									},
									"raw_storage_source_name": {
										"value": {
											"value": "@concat(item().TableName, '.csv')",
											"type": "Expression"
										},
										"type": "string"
									},
									"curated_storage_container": {
										"value": "abfss://odw-curated@pinsodwdatadevstorage.dfs.core.windows.net",
										"type": "string"
									},
									"curated_storage_folder": {
										"value": {
											"value": "@item().LandingFolder",
											"type": "Expression"
										},
										"type": "string"
									},
									"curated_storage_table_name": {
										"value": {
											"value": "@item().FileName",
											"type": "Expression"
										},
										"type": "string"
									}
								},
								"snapshot": true,
								"sparkPool": {
									"referenceName": "pinsodwspark",
									"type": "BigDataPoolReference"
								}
							}
						},
						{
							"name": "Script1",
							"type": "Script",
							"dependsOn": [
								{
									"activity": "py_process_raw_to_curated_table_for_reports",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"linkedServiceName": {
								"referenceName": "ls_sql_builtin",
								"type": "LinkedServiceReference",
								"parameters": {
									"db_name": "odw_reporting_db"
								}
							},
							"typeProperties": {
								"scripts": [
									{
										"parameters": [
											{
												"name": "columns",
												"type": "String",
												"value": "",
												"direction": "Input"
											},
											{
												"name": "location",
												"type": "String",
												"value": "",
												"direction": "Input"
											},
											{
												"name": "TableName",
												"type": "String",
												"value": "",
												"direction": "Input"
											}
										],
										"type": "Query",
										"text": "\nCREATE EXTERNAL TABLE @TableName (\n\t@columns\n\t)\n\tWITH (\n\tLOCATION = @location,\n\tDATA_SOURCE = [odw-curated_pinsodwdatadevstorage_dfs_core_windows_net],\n\tFILE_FORMAT = [SynapseParquetFormat]\n\t)"
									}
								]
							}
						}
					]
				}
			},
			{
				"name": "Truncate logging table",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "ls_odw_sql_mipins",
					"type": "LinkedServiceReference",
					"parameters": {
						"db_name": "MiPINS-PRD-ISS"
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "NonQuery",
							"text": "TRUNCATE table Audit.SynapseLogging ;"
						}
					]
				}
			}
		],
		"folder": {
			"name": "MiPINS Data Copy"
		},
		"annotations": []
	}
}