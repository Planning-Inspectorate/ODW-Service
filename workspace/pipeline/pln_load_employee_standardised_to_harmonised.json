{
	"name": "pln_load_employee_standardised_to_harmonised",
	"properties": {
		"activities": [
			{
				"name": "Lookup Get Notebooks",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "dst_standardised_to_harmonised_pipeline_list",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach Iterate All Notebooks",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Lookup Get Notebooks",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Lookup Get Notebooks').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "py_load_standardised_to_harmonised",
							"type": "SynapseNotebook",
							"dependsOn": [
								{
									"activity": "Set variable Notebookname",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"notebook": {
									"referenceName": {
										"value": "@variables('notebookName')",
										"type": "Expression"
									},
									"type": "NotebookReference"
								},
								"snapshot": true,
								"sparkPool": {
									"referenceName": "pinsodwspark",
									"type": "BigDataPoolReference"
								}
							}
						},
						{
							"name": "Set variable Notebookname",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "notebookName",
								"value": {
									"value": "'https://web.azuresynapse.net/en-us/authoring/analyze/notebooks/py_sap_hr_harmonised_layer1_transform'",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"variables": {
			"notebookName": {
				"type": "String"
			}
		},
		"annotations": []
	}
}