{
	"name": "pln_copy_mipins_TEST",
	"properties": {
		"activities": [
			{
				"name": "Copy tables to ODW_csv_TEST",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_odw_mipins_load_tables",
						"type": "DatasetReference",
						"parameters": {
							"source_schema_name": "live",
							"source_table_name": "dim_appeal_combined"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_odw_mipins_tables_csv",
						"type": "DatasetReference",
						"parameters": {
							"LandingContainer": "odw-raw",
							"LandingFolder": "appeal",
							"Filename": "dim_appeal_combined.csv"
						}
					}
				]
			}
		],
		"folder": {
			"name": "MiPINS Data Copy"
		},
		"annotations": [],
		"lastPublishTime": "2023-01-19T13:06:22Z"
	},
	"type": "Microsoft.Synapse/workspaces/pipelines"
}