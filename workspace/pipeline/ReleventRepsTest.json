				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT\r\nC.CaseReference\r\n,C.CaseUniqueId\r\n,C2.[ID] AS ContactID\r\n,C2.[OrganisationName]\r\n,C2.[JobTitle]\r\n,C2.[FullName]\r\n,C2.[PhoneNumber]\r\n,C2.[EmailAddress]\r\n,C2.[BuildingNumber]\r\n,C2.[Street]\r\n,C2.[Town]\r\n,C2.[County]\r\n,C2.[Country]\r\n,C2.[Postcode]\r\n,RR.[ID] AS RelevantRepID\r\n,RR.[AttendPrelimMeeting]\r\n,RR.[DateReceived]\r\n--,REPLACE(CAST(RR.[Notes] AS NVarchar(MAX)),'%[^0-9a-zA-Z]%',' ') AS Notes\r\n--,REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(CAST(RR.[Notes] AS VARCHAR(MAX)),'.',''),'-',''),'/',''),'\\',''),'\"',''),',',''),'    ',''),'â',''),'€',''),'™',''),'¢',''),';',''),'^',''),'~',''),'¬',''),'`','') AS Notes\r\n,CAST(CAST(RR.[Notes] as XML).value('.','varbinary(max)') AS nvarchar(max)) AS Notes\r\n,RR.[AgentContactId]\r\n,RR.[RepresentAtCompAcqHearing]\r\n,RR.[RepresentAtIssueHearing]\r\n,RR.[RepresentAtOpenFloorHearing]\r\n--,REPLACE(CAST(RR.[RepresentationOriginal] AS NVarchar(MAX)),'%[^0-9a-zA-Z]%',' ') AS RepresentationOriginal\r\n--,REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(CAST(RR.[RepresentationOriginal] AS VARCHAR(MAX)),'.',''),'-',''),'/',''),'\\',''),'\"',''),',',''),'    ',''),'â',''),'€',''),'™',''),'¢',''),';',''),'^',''),'~',''),'¬',''),'`','') AS RepresentationOriginal\r\n,CAST(CAST(RR.[RepresentationOriginal] as XML).value('.','varbinary(max)') AS nvarchar(max)) AS RepresentationOriginal\r\n--,REPLACE(CAST(RR.[RepresentationRedacted] AS NVarchar(MAX)),'%[^0-9a-zA-Z]%',' ') AS RepresentationRedacted\r\n--,REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(CAST(RR.[RepresentationRedacted] AS VARCHAR(MAX)),'.',''),'-',''),'/',''),'\\',''),'\"',''),',',''),'    ',''),'â',''),'€',''),'™',''),'¢',''),';',''),'^',''),'~',''),'¬',''),'`','')  AS RepresentationRedacted\r\n,CAST(CAST(RR.[RepresentationRedacted] as XML).value('.','varbinary(max)') AS nvarchar(max)) AS RepresentationRedacted\r\n,RR.[SubmitLaterReps]\r\n,RR.[WebReference]\r\n,RRS.[Name] AS RelevantRepStatus\r\n,RRL.[OwnerOrOccupier]\r\n,RRL.[PowerToSell]\r\n,RRL.[EntitledToClaim]\r\n,RRL.[Other]\r\n,RRL.[DescriptionIfOther]\r\n,RRB.[Name] AS RelRepOnBehalfOf\r\n,RRO.[Name] AS RelRepOrganisation\r\n,COM.[Name] AS PreferredContactMethod\r\nFROM [otcs].[dbo].[Horizon_RelevantRepresentation] RR\r\nLEFT JOIN [otcs].[dbo].[Horizon_CaseReference] C ON RR.CaseId = C.CaseNodeId\r\nLEFT JOIN [otcs].[dbo].[Horizon_RelevantRepContact] C2 ON RR.ContactId = C2.ID\r\nLEFT JOIN [otcs].[dbo].[Horizon_TypeOfRelevantRepState] RRS ON RR.RepresentationStateId = RRS.ID\r\nLEFT JOIN [otcs].[dbo].[Horizon_RelevantRepInterestInLand] RRL ON RR.InterestInLandId = RRL.ID\r\nLEFT JOIN [otcs].[dbo].[Horizon_TypeOfRelRepOnBehalfOf] RRB ON RR.OnBehalfOfId = RRB.ID\r\nLEFT JOIN [otcs].[dbo].[Horizon_TypeOfRelRepOrganisation] RRO ON RR.OrganisationTypeId = RRO.ID\r\nLEFT JOIN [otcs].[dbo].[Horizon_TypeOfCommunicationPreference] COM ON RR.PreferredContactMethodId = COM.ID",
						"queryTimeout": "02:00:00",
						"isolationLevel": "ReadCommitted",
						"partitionOption": "None"
					},
					"sink": {
