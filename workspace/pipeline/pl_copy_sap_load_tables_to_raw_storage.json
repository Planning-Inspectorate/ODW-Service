{
	"name": "pl_copy_sap_load_tables_to_raw_storage",
	"properties": {
		"activities": [
			{
				"name": "Lookup Get Source Table List",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "dst_source_to_standardised_table_list",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach Itearte All Source Tables",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Lookup Get Source Table List",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Lookup Get Source Table List').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy data from MiPINS Load Tables",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "dst_miPINS_load_tables",
									"type": "DatasetReference",
									"parameters": {
										"source_schema_name": {
											"value": "@item().sourceLocation",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "dst_MiPINS_load_tables_raw_storage",
									"type": "DatasetReference",
									"parameters": {
										"staging_folder_name": {
											"value": "@item().stagingFolderName",
											"type": "Expression"
										},
										"staging_file_name": {
											"value": "@item().sourceName",
											"type": "Expression"
										},
										"staging_container_name": {
											"value": "@item().stagingContainerName",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"annotations": [],
		"lastPublishTime": "2022-02-24T14:51:29Z"
	},
	"type": "Microsoft.Synapse/workspaces/pipelines"
}