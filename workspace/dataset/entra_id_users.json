{
	"name": "entra_id_users",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "users.json"
			}
		},
		"folder": {
			"name": "old-resources"
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('entraid', '/', formatDateTime(utcnow(), 'yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": "odw-raw"
			}
		},
		"schema": {}
	}
}