{
	"name": "b_ds_binary_sink",
	"properties": {
		"description": "alphabetical index",
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Filename": {
				"type": "string"
			},
			"SourceType": {
				"type": "string"
			},
			"SourceSystem": {
				"type": "string"
			},
			"Date": {
				"type": "string"
			}
		},
		"folder": {
			"name": "0_Raw"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().Filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().SourceType,'/',dataset().SourceSystem,'/')\n",
					"type": "Expression"
				},
				"fileSystem": "odw-raw"
			}
		}
	}
}