{
	"name": "ds_datalab_get_saphr_files",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_datalab",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ds_FileLoad_Type": {
				"type": "string"
			},
			"ds_Folder_Path": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Archive/Unused-Dataset"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureFileStorageLocation",
				"fileName": "*.csv",
				"folderPath": {
					"value": "@concat('archive/', dataset().ds_Folder_Path,'/','MONTHLY')",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}