{
    "name": "ds_odwdatalab_source",
    "properties": {
        "linkedServiceName": {
            "referenceName": "ls_datalab",
            "type": "LinkedServiceReference"
        },
        "parameters": {
            "ds_FileLoad_Type": {
                "type": "string"
            },
            "ds_Folder_Path": {
                "type": "string"
            },
            "ds_Env_Type": {
                "type": "string"
            }
        },
        "annotations": [],
        "type": "Binary",
        "typeProperties": {
            "location": {
                "type": "AzureFileStorageLocation",
                "fileName": "*.csv",
                "folderPath": {
                    "value": "@concat('archive/', @concat(dataset().ds_Folder_Path,'/',dataset().ds_Env_Type,'/',dataset().ds_FileLoad_Type))",
                    "type": "Expression"
                }
            }
        },
        "folder": {
            "name": "Archive/Unused"
        }
    }
}