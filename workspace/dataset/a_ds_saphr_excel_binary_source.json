{
	"name": "0_ds_saphr_excel_binary_source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_datalab",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Filename": {
				"type": "string",
				"defaultValue": "Addresses - 20220831.xlsx"
			},
			"Frequency": {
				"type": "string",
				"defaultValue": "Monthly"
			},
			"Date": {
				"type": "string",
				"defaultValue": "2022-08"
			}
		},
		"folder": {
			"name": "0_Raw"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureFileStorageLocation",
				"fileName": {
					"value": "@dataset().Filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('ODW/HR/',dataset().Frequency,'/',dataset().Date)",
					"type": "Expression"
				}
			}
		}
	}
}