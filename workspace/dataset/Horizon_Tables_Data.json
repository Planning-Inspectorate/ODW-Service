				},
				"fileSystem": "odw-raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Column_1",
				"type": "String"
			},
			{
				"name": "Column_2",
				"type": "String"
			}
		]
	}
}