{
    "name": "Horizon_Tables_Data",
    "properties": {
        "linkedServiceName": {
            "referenceName": "ls_storage",
            "type": "LinkedServiceReference"
        },
        "parameters": {
            "FileName": {
                "type": "string",
                "defaultValue": "SpecialistCoastalAccess.csv"
            }
        },
        "annotations": [],
        "type": "DelimitedText",
        "typeProperties": {
            "location": {
                "type": "AzureBlobFSLocation",
                "fileName": {
                    "value": "@dataset().FileName",
                    "type": "Expression"
                },
                "folderPath": {
                    "value": "@concat('archive/', @concat('Horizon', '/', formatDateTime(utcnow(), 'yyyy-MM-dd')))",
                    "type": "Expression"
                },
                "fileSystem": "odw-raw"
            },
            "columnDelimiter": ",",
            "escapeChar": "\\",
            "firstRowAsHeader": true,
            "quoteChar": "\""
        },
        "schema": [],
        "folder": {
            "name": "Archive/Unused"
        }
    },
    "type": "Microsoft.Synapse/workspaces/datasets"
}