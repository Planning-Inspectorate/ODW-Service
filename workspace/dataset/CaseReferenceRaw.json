{
	"name": "CaseReferenceRaw",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "CaseReference.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
