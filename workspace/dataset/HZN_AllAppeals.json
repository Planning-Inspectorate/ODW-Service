{
	"name": "HZN_AllAppeals",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "AllAppeals.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('Horizon', '/',utcNow('d'))",
					"type": "Expression"
				},
				"fileSystem": "odw-raw"
			},
			"columnDelimiter": ",",
			"compressionCodec": "snappy",
			"compressionLevel": "Optimal",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}