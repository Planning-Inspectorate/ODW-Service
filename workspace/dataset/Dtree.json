{
	"name": "Dtree",
	"properties": {
		"description": "storing Dtree data in raw folder",
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "DTree.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('archive/', 'Horizon', '/', formatDateTime(utcnow(), 'yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": "odw-raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [],
		"folder": {
			"name": "archive/Archive/Unused"
		}
	}
}