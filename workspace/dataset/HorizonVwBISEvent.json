			},
			{
				"name": "chart_status",
				"type": "String"
			},
			{
				"name": "dim_event_date_key",
				"type": "String"
			},
			{
				"name": "hzn_event_date",
				"type": "String"
			},
			{
				"name": "hzn_event_end_date",
				"type": "String"
			},
			{
				"name": "actual_duration",
				"type": "String"
			},
			{
				"name": "est_prep_time",
				"type": "String"
			},
			{
				"name": "est_sit_time",
				"type": "String"
			},
			{
				"name": "est_rep_time",
				"type": "String"
			}
		]
	}
}