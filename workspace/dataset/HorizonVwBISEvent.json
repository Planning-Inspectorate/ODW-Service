				},
				"fileSystem": "odw-raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "appeal_reference_number",
				"type": "String"
			},
			{
