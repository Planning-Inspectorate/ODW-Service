{
	"name": "DaRT_Inspectors_Entity_Sink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "S62AViewCases.csv"
			}
		},
		"folder": {
			"name": "Archive/Unused"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('archive/Horizon/', formatDateTime(utcnow(), 'yyyy-MM-dd'))\n",
					"type": "Expression"
				},
				"fileSystem": "odw-raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}