				"source": [
					"def create_spark_dataframe() -> DataFrame:\r\n",
					"    spark_dataframe: DataFrame = spark.createDataFrame([], schema=create_spark_schema(db_name, entity_name))\r\n",
					"    return spark_dataframe"
				],
