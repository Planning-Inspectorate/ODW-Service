				"source": [
					"#CONNECTION_STR = \"\"\n",
					"FULLY_QUALIFIED_NAMESPACE = \"pins-sb-odw-dev-uks-b9rt9m.servicebus.windows.net\"\n",
					"TOPIC_NAME = \"employee\"\n",
					"credential = DefaultAzureCredential(managed_identity_client_id='')"
				],
				"execution_count": 70
