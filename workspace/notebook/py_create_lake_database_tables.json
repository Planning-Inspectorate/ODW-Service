					"            300,\n",
					"            {\n",
					"                \"db_name\": db_name,\n",
					"                \"entity_name\": table,\n",
					"                \"is_servicebus_schema\": table not in NON_SERVICE_BUS_ENTITIES\n",
					"            }\n",
					"        )"
				],
