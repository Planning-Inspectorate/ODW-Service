					"# Create tables"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"ALL_DATA_TABLES = STANDARDISED_TABLES | HARMONISED_TABLES | CURATED_TABLES\n",
					"ALL_DATA_TABLES = {\n",
					"    \"odw_standardised_db\": STANDARDISED_TABLES,\n",
					"    \"odw_harmonised_db\": HARMONISED_TABLES,\n",
					"    \"odw_curated_db\": CURATED_TABLES\n",
					"}\n",
					"for db_name, tables in ALL_DATA_TABLES.items():\n",
					"    for table in tables:\n",
					"        mssparkutils.notebook.run(\n",
					"            \"service-bus/create_table_from_schema\",\n",
					"            300,\n",
					"            {\n",
					"                \"db_name\": db_name,\n",
					"                \"entity_name\": table,\n",
					"                \"is_servicebus_schema\": table not in NON_SERVICE_BUS_ENTITIES\n",
					"            }\n",
					"        )"
				],
				"execution_count": null
			},
