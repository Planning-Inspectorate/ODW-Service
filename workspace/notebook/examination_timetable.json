				"source": [
					"%%sql\r\n",
					"\r\n",
					"CREATE OR REPLACE VIEW odw_curated_db.vw_examination_timetable\r\n",
					"\r\n",
					"AS\r\n",
					"\r\n",
					"SELECT DISTINCT\r\n",
					"\r\n",
					"    ET.CaseReference                                            AS caseReference,\r\n",
					"    ET.ODTEventID                                               AS eventID,\r\n",
					"    ET.TypeOfExamination                                        AS eventType,\r\n",
					"    ET.Name                                                     AS eventTitle,\r\n",
					"    CAST(ET.DeadlineStartDateTime AS date)                      AS eventDeadlineStartDate,\r\n",
					"    CAST(ET.Date AS date)                                       AS eventDate,\r\n",
					"    ET.EventLineItems                                           AS eventLineItems,\r\n",
					"    ET.Description                                              AS eventLineItemDescription \r\n",
					"    \r\n",
					"FROM odw_harmonised_db.casework_nsip_examination_timetable_dim  AS ET\r\n",
					"WHERE ET.IsActive = 'Y'\r\n",
					"ORDER BY caseReference,eventTitle DESC\r\n",
					""
				],
				"execution_count": 13
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
