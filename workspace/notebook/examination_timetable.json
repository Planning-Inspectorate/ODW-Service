				"source": [
					"%%sql\r\n",
					"CREATE OR REPLACE TABLE odw_curated_db.examination_timetable\r\n",
					"USING delta\r\n",
					"AS\r\n",
					"SELECT \r\n",
					"    caseReference,\r\n",
					"    eventID,\r\n",
					"    eventType,\r\n",
					"    eventTitle,\r\n",
