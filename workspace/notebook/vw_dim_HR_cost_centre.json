					"%%sql\n",
					"\n",
					"\n",
					"\n",
					"CREATE VIEW IF NOT EXISTS odw_curated_db.vw_dim_HR_cost_centre  AS\n",
					"SELECT\n",
					"\t   RANK() OVER(ORDER BY a.CostCentreCode) AS dim_cost_centre_key\n",
					"\t  ,a.*\n",
					"FROM\n",
					"(\n",
					"\tSELECT DISTINCT \n",
					"\t\t  CASE WHEN CostCtr ='' THEN '-1' ELSE CostCtr END AS CostCentreCode\n",
					"\t\t ,CASE WHEN CostCentre ='' THEN '-' ELSE CostCentre END AS CostCentre\n",
					"\tFROM odw_harmonised_db.hist_sap_hr\n",
					") a ;"
				],
				"execution_count": 8
			},
