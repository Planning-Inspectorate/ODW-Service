				"source": [
					"%%sql\r\n",
					"\r\n",
					"-- Build payband_dim table  - table is incorrect\r\n",
					"\r\n",
					"INSERT OVERWRITE odw_harmonised_db.employeegroup_dim (\r\n",
					"    EmployeeGroupID,\r\n",
					"    EEGrp,\r\n",
					"    EmployeeGroup,\r\n",
					"    SourceSystemID,\r\n",
					"    StartDate,\r\n",
					"    EndDate,\r\n",
					"    IngestionDate,\r\n",
					"    ValidTo,\r\n",
					"    RowID,\r\n",
					"    IsActive\r\n",
					")\r\n",
					"\r\n",
					"SELECT \r\n",
					"    Row_Number() over (order by T1.EEGrp) AS EmployeeGroupID,\r\n",
					"    T1.EEGrp,\r\n",
					"    FIRST(T1.Employee_Group), \r\n",
					"    FIRST(T2.SourceSystemID) as SourceSystemID,\r\n",
					"    CURRENT_TIMESTAMP AS StartDate,\r\n",
					"    NULL AS EndDate,\r\n",
					"    CURRENT_TIMESTAMP AS IngestionDate,\r\n",
					"    NULL AS ValidTo,\r\n",
					"    md5(concat(IFNULL(T1.EEGrp,'.'), IFNULL(FIRST(T1.Employee_Group),'.'))) as RowID,\r\n",
					"    'Y' as IsActive\r\n",
					"\r\n",
					"FROM odw_standardised_db.vw_saphr_historic T1\r\n",
					"LEFT JOIN odw_harmonised_db.sourcesystem_fact T2 ON \"SAP HR\" = T2.Description and T2.IsActive = 'Y'\r\n",
					"WHERE T1.EEGrp IS NOT NULL\r\n",
					"GROUP BY T1.EEGrp\r\n",
					"\r\n",
					"\r\n",
					";\r\n",
					""
				],
				"execution_count": 4
			}
		]
	}
