				"source": [
					"%%sql\n",
					"CREATE OR REPLACE VIEW odw_curated_db.vw_legacy_folder_data\n",
					"AS\n",
					"SELECT DISTINCT\n",
					"    CAST(folder.ID AS INT) AS id\n",
					"    ,proj.CaseReference AS caseReference\n",
					"    ,folder.DisplayNameEnglish AS displayNameEnglish\n",
					"    ,folder.DisplayNameWelsh AS displayNameWelsh\n",
					"    ,CASE\n",
					"        WHEN ParentFolderID = CaseNodeId THEN \n",
					"            NULL\n",
					"        ELSE \n",
					"            CAST(folder.ParentFolderID AS INT)     \n",
					"        END AS parentFolderId\n",
					"    ,CASE folder.caseStage\n",
					"        WHEN 'Post decision' THEN \n",
					"            'post_decision'\n",
					"        ELSE \n",
					"            Lower(folder.caseStage)\n",
					"    END AS caseStage\n",
					"FROM \n",
					"    odw_harmonised_db.horizon_folder AS folder\n",
					"    INNER JOIN odw_curated_db.nsip_project as proj\n",
					"        ON folder.CaseReference = proj.caseReference\n",
					""
				],
				"execution_count": 2
			},
