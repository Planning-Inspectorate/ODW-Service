					"%%sql\r\n",
					"\r\n",
					"-- INSERT OVERWRITE odw_harmonised_db.mrw_lines_dim (\r\n",
					"--     MRWLinesID,\r\n",
					"--     InspectorID,\r\n",
					"--     TimeSHeetDate,\r\n",
					"--     LineNumber,\r\n",
					"--     JobID,\r\n",
					"--     WorkstageID,\r\n",
					"--     WorkstageName,\r\n",
					"--     Dated,\r\n",
					"--     Hours,\r\n",
					"--     PDACETLDate,\r\n",
					"--     StartDate,\r\n",
					"--     EndDate,\r\n",
					"--     IngestionDate,\r\n",
					"--     ValidTo,\r\n",
					"--     RowID,\r\n",
					"--     IsActive\r\n",
					"-- )\r\n",
					"\r\n",
					"SELECT\r\n",
					"    id AS MRWLinesID,\r\n",
					"    inspector_id AS InspectorID,\r\n",
					"    timesheet_date AS TimeSHeetDate,\r\n",
					"    line_number AS LineNumber,\r\n",
					"    job_id AS JobID,\r\n",
					"    workstage_id AS WorkstageID,\r\n",
					"    workstage_name AS WorkstageName,\r\n",
					"    dated AS Dated,\r\n",
					"    hours AS Hours,\r\n",
					"    pdac_etl_date AS PDACETLDate,\r\n",
					"    CURRENT_TIMESTAMP AS StartDate, \r\n",
					"    NULL AS EndDate,\r\n",
					"    ingested_datetime AS IngestionDate,\r\n",
					"    NULL AS ValidTo,\r\n",
					"    md5(concat(T1.id, T1.inspector_id, T1.line_number)) as RowID,\r\n",
					"    \"Y\" AS IsActive\r\n",
					"FROM odw_standardised_db.legacy_timesheets_mwr_lines T1;\r\n",
					""
				],
