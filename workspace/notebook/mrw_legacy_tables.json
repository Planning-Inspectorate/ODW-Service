					"    JobID,\r\n",
					"    WorkstageID,\r\n",
					"    WorkstageName,\r\n",
					"    Dated,\r\n",
					"    Hours,\r\n",
					"    PDACETLDate,\r\n",
					"    StartDate,\r\n",
					"    EndDate,\r\n",
					"    IngestionDate,\r\n",
					"    ValidTo,\r\n",
					"    RowID,\r\n",
					"    IsActive\r\n",
					")\r\n",
					"\r\n",
					"SELECT\r\n",
					"    odw_standarised_db.legacy_timesheets_mrw_line.id AS MRWLinesID,\r\n",
					"    inspector_id AS InspectorID,\r\n",
					"    timesheet_date AS TimeSHeetDate,\r\n",
					"    line_number AS LineNumber,\r\n",
					"    Job_id AS JobID,\r\n",
					"    WorkstageID,\r\n",
					"    WorkstageName,\r\n",
					"    Dated,\r\n",
					"    Hours,\r\n",
					"    PDACETLDate,\r\n",
					"    StartDate,\r\n",
					"    EndDate,\r\n",
					"    IngestionDate,\r\n",
					"    ValidTo,\r\n",
					"    RowID,\r\n",
					"    IsActive,\r\n",
					"    \"SAP HR\" AS Description,\r\n",
					"    CURRENT_TIMESTAMP AS StartDate, \r\n",
					"    NULL AS EndDate,\r\n",
					"    CURRENT_TIMESTAMP AS IngestionDate,\r\n",
					"    NULL AS ValidTo,\r\n",
					"\r\n",
					"\r\n",
					"    md5(\"SAP HR\") as RowID"
				],
