				"source": [
					"%%sql\n",
					"\n",
					"CREATE VIEW IF NOT EXISTS odw_curated_db.vw_dim_HR_Personnel_SubArea AS\n",
					"\n",
					"SELECT\n",
					"\t\tRANK() OVER(ORDER BY a.PersonnelSubAreaCode, a.PersonnelSubArea) AS dim_personnel_subarea_key\n",
					"\t   ,a.*\n",
					"FROM\n",
					"(\n",
					"\tSELECT DISTINCT\n",
					"\t\t\tPSubarea AS PersonnelSubAreaCode\n",
					"\t\t   ,CASE PSubarea  WHEN 'P210' THEN 'Digital' \n",
					"\t\t\t\t\t\t   WHEN 'P205' THEN 'Perf Com & Ops' \n",
					"\t\t\t\t\t\t   WHEN 'P515' THEN 'EST/Central Ops'\n",
					"\t\t\t\t\t\t   WHEN 'P610' THEN 'Knowledge'\n",
					"\t\t\tELSE PersonnelSubarea\n",
					"\t\t\tEND AS PersonnelSubArea\n",
					"\tFROM odw_harmonised_db.hist_SAP_HR\t  \n",
					") a "
				],
				"execution_count": 1
			},
			{
				"cell_type": "code",
				"metadata": {
					"microsoft": {
						"language": "sparksql"
					},
					"collapsed": false
				},
				"source": [
					"%%sql\n",
					"create or replace table odw_curated_db.pbi_dim_HR_Personnel_SubArea\n",
					"using delta\n",
					"as\n",
					"SELECT * FROM odw_curated_db.vw_dim_HR_Personnel_SubArea"
				],
