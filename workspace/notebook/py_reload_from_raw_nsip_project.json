				"source": [
					"def cast_columns(df: DataFrame, schema: StructType) -> DataFrame:\r\n",
					"    for field in schema.fields:\r\n",
					"        df = df.withColumn(field.name, col(field.name).cast(field.dataType))\r\n",
					"    return df"
				],
				"execution_count": null
