				"source": [
					"%%sql\r\n",
					"drop table if exists odw_workspace_db.vw_SAP_HR_email_weekly_layer1_harmonised;\r\n",
					"create table odw_workspace_db.vw_SAP_HR_email_weekly_layer1_harmonised as \r\n",
					"SELECT\r\n",
					"\t Pers_No as PINS_Join_staff_number\r\n",
					"\t ,email_address\r\n",
					"\t ,SW.PDAC_ETL_Date\r\n",
					" FROM odw_standardised_db.vw_SAP_HR_email_weekly SW\r\n",
					" where SW.PDAC_ETL_Date > (select LastRun from odw_config_db.pipeline_runs where sourceName ='vw_SAP_HR_email_weekly' )"
				],
				"execution_count": 19
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"microsoft": {
						"language": "sparksql"
					}
				},
				"source": [
					"%%sql\r\n",
					"merge into odw_config_db.pipeline_runs target\r\n",
					"using (select 'vw_SAP_HR_email_weekly' as source_name , max(PDAC_ETL_Date ) as max_PDAC_ETL_Date from odw_workspace_db.sap_hr_layer1_harmonised ) updates\r\n",
					"on target.sourceName=updates.source_name\r\n",
					"WHEN MATCHED THEN\r\n",
					"  UPDATE SET target.LastRun = updates.max_PDAC_ETL_Date\r\n",
					"WHEN NOT MATCHED\r\n",
					"  THEN INSERT (sourceName,LastRun) VALUES (source_name, max_PDAC_ETL_Date)"
				],
