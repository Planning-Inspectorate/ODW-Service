					"OrganisationUnit        AS OrganisationUnitCode,\r\n",
					"Description             AS OrganisationUnitDesc,\r\n",
					"IsActive                AS IsActive,\r\n",
					"ValidFrom               AS ValidFrom,\r\n",
					"ValidTo                 AS ValidTo\r\n",
					"\r\n",
					"FROM odw_harmonised_db.hr_organisation_unit_dim\r\n",
