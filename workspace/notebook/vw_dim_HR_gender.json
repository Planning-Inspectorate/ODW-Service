				"source": [
					"%%sql\n",
					"\n",
					"CREATE VIEW IF NOT EXISTS odw_curated_db.vw_dim_HR_Gender AS \n",
					"SELECT DISTINCT \n",
					"    CASE GenderKey \n",
					"        WHEN 'Male' THEN 1 \n",
					"        WHEN 'Female' THEN 2 \n",
					"        ELSE 0 \n",
					"    END AS dim_gender_key,\n",
					"    CASE GenderKey \n",
					"        WHEN '' THEN 'Unknown' \n",
					"        ELSE GenderKey \n",
					"    END AS Gender\n",
					"FROM odw_harmonised_db.hist_SAP_HR"
				],
				"execution_count": 2
			},
