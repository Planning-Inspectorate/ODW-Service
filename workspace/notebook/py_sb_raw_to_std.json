					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"create_dataframe_to_load(table_name)\r\n",
					"apply_df_to_table(table_df, table_name.split('.')[0], table_name.split('.')[1])"
