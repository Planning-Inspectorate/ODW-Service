				"source": [
					"%%sql\r\n",
					"CREATE OR REPLACE TEMPORARY VIEW zendesk_role\r\n",
					"    AS\r\n",
					"\r\n",
					"SELECT\r\n",
					"    ticket_id,\r\n",
					"    name,\r\n",
					"    email,\r\n",
					"    role\r\n",
					"FROM odw_harmonised_db.zendesk_submitter \r\n",
					"UNION\r\n",
					"SELECT\r\n",
					"    ticket_id,\r\n",
					"    name,\r\n",
					"    email,\r\n",
					"    role\r\n",
					"FROM odw_harmonised_db.zendesk_assignee\r\n",
					"UNION \r\n",
					"SELECT\r\n",
					"    ticket_id,\r\n",
					"    name,\r\n",
					"    email,\r\n",
					"    role\r\n",
					"FROM odw_harmonised_db.zendesk_collaborator\r\n",
					"UNION \r\n",
					"SELECT\r\n",
					"    ticket_id,\r\n",
					"    name,\r\n",
					"    email,\r\n",
					"    role\r\n",
					"FROM odw_harmonised_db.zendesk_requester\r\n",
					"order by ticket_id\r\n",
					"\r\n",
					"\r\n",
					"\r\n",
					""
