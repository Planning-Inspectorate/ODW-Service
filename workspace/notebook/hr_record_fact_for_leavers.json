				"source": [
					"%%sql\n",
					"\n",
					"-- CREATE OR REPLACE TEMPORARY VIEW record_fact_from_leavers\n",
					"\n",
					"--     AS\n",
					"\n",
					"SELECT DISTINCT\n",
					"    Pers_No,\n",
					"    Last_name,\n",
					"    First_name,\n",
					"    CoCd,\n",
					"    Company_Code,\n",
					"    Loc,\n",
					"    Location,\n",
					"    PS_group,\n",
					"    Pay_Band_Description,\n",
					"    Org_unit,\n",
					"    Organizational_Unit,\n",
					"    PA,\n",
					"    Personnel_Area,\n",
					"    Personnel_Subarea,\n",
					"    WorkC,\n",
					"    Work_Contract,\n",
					"    to_timestamp(Org_Start_Date) AS Org_Start_Date,\n",
					"    Leaving,\n",
					"    Act,\n",
					"    Action_Type,\n",
					"    ActR,\n",
					"    Reason_for_Action,\n",
					"    S,\n",
					"    Employment_Status,\n",
					"    Employee_No,\n",
					"    Position,\n",
					"    Position_1,\n",
					"    Annual_salary,\n",
					"    Curr,\n",
					"    User_ID,\n",
					"    Email_Address,\n",
					"    Pers_No_1,\n",
					"    Name_of_Manager_OM,\n",
					"    Manager_Position,\n",
					"    MAnager_Position_Text,\n",
					"    LM_E_mail,\n",
					"    ingested_datetime,\n",
					"    expected_from,\n",
					"    expected_to\n",
					"\n",
					"FROM odw_standardised_db.vw_leavers_temp\n",
					"WHERE Pers_No NOT IN (SELECT EmployeeID FROM odw_harmonised_db.hr_record_fact) AND RN = 1;"
				],
				"execution_count": null
			},
