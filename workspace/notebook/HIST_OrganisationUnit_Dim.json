				"source": [
					"%%sql\r\n",
					"-- Build OrganisationUnit_dim table\r\n",
					"\r\n",
					"INSERT OVERWRITE odw_harmonised_db.hr_organisation_unit_dim (\r\n",
					"    OrganisationUnitID,\r\n",
					"    OrganisationUnit,\r\n",
					"    Description,\r\n",
					"    SourceSystemID,\r\n",
					"    IngestionDate,\r\n",
					"    ValidTo,\r\n",
					"    RowID,\r\n",
					"    IsActive\r\n",
					")\r\n",
					"\r\n",
					"SELECT\r\n",
					"    Row_Number() over (Order By T1.Org_Unit) AS OrganisationUnitID,\r\n",
					"    IFNULL(T1.Org_unit, 'No Value') as OrganisationUnit,\r\n",
					"    First(IFNULL(T1.Organizational_Unit, 'No Value')) AS Description,\r\n",
					"    First(T2.SourceSystemID) as SourceSystemID,\r\n",
					"    First(to_timestamp(T1.expected_from)) AS IngestionDate,\r\n",
					"    NULL AS ValidTo,md5(concat(IFNULL(T1.Org_unit,'.'), First(IFNULL(T1.Organizational_Unit, 'No Value')))) as RowID, -- check this\r\n",
					"    'Y' as IsActive\r\n",
					"\r\n",
					"FROM odw_standardised_db.vw_saphr_historic T1 \r\n",
					"LEFT JOIN odw_harmonised_db.main_sourcesystem_fact T2 ON \"SAP HR\" = T2.Description AND T2.IsActive = 'Y'\r\n",
					"WHERE T1.Org_Unit is not null\r\n",
					"GROUP BY T1.Org_unit"
				],
				"execution_count": 16
			},
