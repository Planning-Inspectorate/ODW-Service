				"source": [
					"%%sql\n",
					"\n",
					"CREATE OR REPLACE VIEW odw_curated_db.vw_nsip_subscription\n",
					"AS\n",
					"SELECT DISTINCT\n",
					"    subs.subscriptionId\n",
					"    ,subs.caseReference\n",
					"    ,subs.emailAddress\n",
					"    ,subs.subscriptionType\n",
					"    ,subs.startDate\n",
					"    ,subs.endDate\n",
					"    ,subs.language\t\n",
					"FROM \n",
					"    odw_harmonised_db.sb_nsip_subscription as subs\n",
					"    INNER JOIN odw_curated_db.nsip_project as proj\n",
					"        ON subs.caseReference = proj.caseReference\n",
					"WHERE \n",
					"    subs.IsActive = 'Y'"
				],
				"execution_count": 2
			},
