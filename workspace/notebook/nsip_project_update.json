				"source": [
					"\n",
					"%%sql\n",
					"\n",
					"CREATE OR REPLACE VIEW odw_curated_db.vw_nsip_project_update\n",
					"AS\n",
					"SELECT DISTINCT\n",
					"    pu.id\n",
					"    ,pu.caseReference\n",
					"    ,pu.updateDate\n",
					"    ,pu.updateName\n",
					"    ,pu.updateContentEnglish\n",
					"    ,pu.updateContentWelsh\n",
					"    ,pu.updateStatus\n",
					"FROM \n",
					"    odw_harmonised_db.sb_nsip_project_update as pu\n",
					"    inner join odw_curated_db.nsip_data as project\n",
					"    on pu.caseReference = project.caseReference\n",
					"WHERE \n",
					"    pu.IsActive = 'Y'"
				],
				"execution_count": 1
			},
